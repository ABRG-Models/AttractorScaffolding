@ARTICLE{Bautista2008,
  author = {Bautista, Amando and Garc{\'i}a-Torres, Esmeralda and Mart{\'i}nez-G{\'o}mez,
	Margarita and Hudson, Robyn},
  title = {Do newborn domestic rabbits Oryctolagus cuniculus compete for thermally
